
pipeline {
    agent any
  
    tools {
      maven 'M2_HOME'
      
      }
   
    stages {
      stage('CheckOut') {
        steps {
          echo 'Checkout the source code from GitHub'
          git branch: 'main', url: 'https://github.com/Fatima-Ishrath021/Banking-finance-project.git'
              } }
      
      stage('Package the Application') {
        steps {
          echo " Packaing the Application"
          sh 'mvn clean package'
        }}
    
 
        }
        }
